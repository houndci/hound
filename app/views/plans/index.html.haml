= render "settings"

:javascript
  Hound.settings.userEmailAddress = "#{current_user.billable_email}"

= javascript_pack_tag("notify_tier_change")
= react_component("NotifyTierChange",
  authenticity_token: form_authenticity_token,
  next_tier: plan_selector.next_plan,
  plans: @plans,
  repo_id: @repo.id,
  repo_name: @repo.name,
  user_has_card: current_user.card_exists?,
  marketplace_upgrade_url: @marketplace_upgrade_url,
)
